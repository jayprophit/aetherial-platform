# AI Infrastructure Configuration

# Qdrant Vector Database
QDRANT_URL=http://qdrant:6333
QDRANT_COLLECTION=aetherial_embeddings

# Redis (Feature Store)
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# Model Serving
TORCHSERVE_HOST=torchserve
TORCHSERVE_MANAGEMENT_PORT=8081
TORCHSERVE_INFERENCE_PORT=8080
TORCHSERVE_METRICS_PORT=8082

# Monitoring
PROMETHEUS_URL=http://prometheus:9090
GRAFANA_URL=http://localhost:3000
GRAFANA_ADMIN_PASSWORD=changeme

# AWS Credentials (for model storage)
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_REGION=us-west-2
AWS_S3_BUCKET=your-model-bucket

# API Keys
OPENAI_API_KEY=your_openai_key
ANTHROPIC_API_KEY=your_anthropic_key
GOOGLE_AI_KEY=your_google_ai_key
